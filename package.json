    "gulp-newer": "^1.4.0",
    "gulp-pixrem": "^1.0.0",
    "gulp-postcss": "^7.0.1",
    "gulp-prettier-eslint": "^1.1.0",
    "gulp-rename": "^1.2.3",
    "gulp-sass": "^4.0.1",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sort": "^2.0.0",
    "gulp-stylelint": "^7.0.0",
    "gulp-wp-pot": "^2.3.2",
    "prettier": "^1.12.1",
